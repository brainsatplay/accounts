  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/mongoose": "^5.11.97",
    "build-dev": "^0.0.10",
    "esbuild-register": "^3.3.1",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@giveback007/util-lib": "^0.23.1",
    "cors": "^2.8.5",
